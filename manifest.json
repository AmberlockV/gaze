{
  "manifest_version": 2,
  "name": "Gaze",
  "description": "Web navigation via eye movement",
  "version": "1.0.1",

  "permissions": [
      "*://*/*",
      "tabs"
  ],

  "icons": {
    "16" : "src/owl.png"
    "48" : "src/owl.png",
    "128" : "src/owl.png"
  },

  "browser_action" : {
      "default_icon": "src/owl-gray.png"
  },

  "web_accessible_resources": [
       "lib/jquery.min.map",
       "lib/jquery-1.10.2.min.js",
       "build/camgaze.js",
       "build/compatibility.js",
       "build/jsfeat.js",
       "cascades/eye.js",
       "cascades/frontalface.js",
       "src/elements.js",
       "src/app.js"
  ],

  "background": {
    "scripts": [
        "src/background.js"
    ]
  }
}

