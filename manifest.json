{
  "manifest_version": 1,
  "name": "In Sight",
  "description": "Web navigation via gaze prediction",
  "version": "1.0.0",

  "permissions": [
    "http://*.com/*"
  ],

  "content_scripts": [
    {
      "css": [
        "src/style.css"
      ],
      "js": [
        "build/camgaze.js",
        "build/compatibility.js"<
        "build/jsfeat.js",
        "cascades/eye.js",
        "cascades/frontalface.js",
        "src/elements.js",
        "src/app.js",
        "libs/jquery-2.1.1.min.js",
        "libs/locache.min.js",
        "src/better_recs.js",
        "src/utils.js",
        "src/templates.js",
        "src/data.js",
        "src/app.js",
        "src/detail_view.js",
        "src/list_view.js",
        "src/genre_view.js"
      ],
      "run_at": "document_end"
    }
  ]
}

